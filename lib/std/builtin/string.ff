/*
 * Calls to these functions are emitted by the compiler for basic string
 * operations. For user-side string manipulation, see std.string instead.
 */

__builtin_decl_mangled("copy", null, &str, &str)
__builtin_decl_mangled("drop", null, &str)
__builtin_decl_mangled("len", i64, &str)

fn len(__s: str) -> i64 {
	ret len(&__s)
}
